<mapper namespace="chat.dao.chatDAO">

	<select id="getLoanInfo" parameterType="String" resultType="laonInfoVO">
		select name, changedt, baserate, addrate, finalrate 
		from hana_loan 
		where name=#{name}
	</select>
</mapper>